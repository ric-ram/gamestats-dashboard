{
	"files": [],
	"references": [
		{
			"path": "apps/backend"
		},
		{
			"path": "apps/frontend"
		}
	],
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@backend/*": [
				"apps/backend/src/*"
			],
			"@frontend/*": [
				"apps/frontend/src/*"
			]
		},
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"useDefineForClassFields": false
	},
	"skipLibCheck": true,
}